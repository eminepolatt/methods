
import java.util.Scanner;


public class besvealtıyaBolunebilme {
    
    public static void main(String[] args) {
      Scanner klavye=new Scanner(System.in);
        System.out.println("Bir sayı giriniz");
    int   sayi=klavye.nextInt();
    if(sayi%5==0 && sayi%6==0){
        System.out.println("sayi 5ve 6 ya bölünür");
    }
    else if(sayi%5==0){
        System.out.println("Sayı sadece 5 e bölünür");
    }
    else if(sayi%6==0){
        System.out.println("sayı sadece 6 ya bölünür");
    }
    else {
        System.out.println("sayı ikisine de bölünmez");
    }
}
}
