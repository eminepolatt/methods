
public class nkadarAdyazan {
    public static void main(String[] args) {
        // kendisine parametre olarak gelen n tam sayıyı kadar adınızı ekrana yazan programı ve metodu yazını
        int x;
        Scanner k=new Scanner(System.in);
        x=k.nextInt();
        yaz(x);
              
    }
    public static int yaz(int a){
        for (int i = 0; i <a; i++) {
            System.out.println("Emine");
        }
        return (a);
       
       
    
}
}       
