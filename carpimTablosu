
public class carpımTablosu {
   
    public static void main(String[] args) {
        // Çarpım tablosu
        int carpim,i,j;
        for ( i = 1; i <= 10; i++) {
            for ( j = 1; j <= 10; j++) {
              carpim=i*j;
                System.out.println(i+"*"+j+"="+carpim+"\t");            }
             System.out.println("________________");
        }
        System.out.println(" ");
       
    } 
}
