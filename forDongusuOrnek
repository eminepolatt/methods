
import java.util.Scanner;


public class forDongusuOrnek {
     public static void main(String[] args) {
        // klavyeden girilen say覺lar toplanacakt覺r.0 girince duran ve sonucu ekrana yazd覺ran program
   int t,toplam=0;
   Scanner k=new Scanner(System.in);
       
for(;;){
    System.out.println("bir say覺 giriniz");
      t=k.nextInt();   
      if(t==0){
          System.out.println(toplam);System.exit(1);
      }
      toplam+=t;
   }
   
    }
}
