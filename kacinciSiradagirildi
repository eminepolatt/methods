
import java.util.Scanner;

public class kacıncıSıradagirildi {
      public static void main(String[] args) {
        //
        Scanner k=new Scanner(System.in);
        int sayi;
        for(int i=0;i<10;i++){
            System.out.println("bir sayı giriniz");
            sayi=k.nextInt();
            if(sayi==5){
                System.out.println(i+1+".sırada girildi");
            }
        }
    }
}
